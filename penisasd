KillPlayer.MouseButton1Click:Connect(function()
	Kill(GetPlayer(PlayerName.Text))
end)

States.LoopKills_Neutral = false
States.LoopKills_Criminal = false
States.LoopKills_Inmate = false
States.LoopKills_Guard = false
States.KillAura = false
States.KillAura_Neutral = false
States.KillAura_Criminal = false
States.KillAura_Guard = false
States.KillAura_Inmate = false

LoopKillNeutral.MouseButton1Click:Connect(function()
	if States.LoopKills_Neutral == true then
		States.LoopKills_Neutral = false
		LoopKillNeutral.Text = "X"
	else
		States.LoopKills_Neutral = true
		LoopKillNeutral.Text = ""
	end
	while wait() do
		if States.LoopKills_Neutral then
			for i,v in pairs(game.Teams.Neutral:GetPlayers()) do
				pcall(function()
					if v ~= game.Players.LocalPlayer then
						if v.Character.Humanoid.Health > 0 then
							Kill(v)
						end
					end
				end)
			end
		end
	end
end)

LoopKillCriminal.MouseButton1Click:Connect(function()
	if States.LoopKills_Criminal == true then
		States.LoopKills_Criminal = false
		LoopKillCriminal.Text = "X"
	else
		States.LoopKills_Criminal = true
		LoopKillCriminal.Text = ""
	end
	while wait() do
		if States.LoopKills_Criminal then
			for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
				pcall(function()
					if v ~= game.Players.LocalPlayer then
						if v.Character.Humanoid.Health > 0 then
							Kill(v)
						end
					end
				end)
			end
		end
	end
end)

