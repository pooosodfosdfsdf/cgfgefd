LoopKillInmate.MouseButton1Click:Connect(function()
	if States.LoopKills_Inmate == true then
		States.LoopKills_Inmate = false
		LoopKillInmate.Text = "X"
	else
		States.LoopKills_Inmate = true
		LoopKillInmate.Text = ""
	end
	while wait() do
		if States.LoopKills_Inmate then
			for i,v in pairs(game.Teams.Inmates:GetPlayers()) do
				pcall(function()
					if v ~= game.Players.LocalPlayer then
						if v.Character.Humanoid.Health > 0 then
							Kill(v)
						end
					end
				end)
			end
		end
	end
end)

LoopKillPlayer.MouseButton1Click:Connect(function()
	local player = GetPlayer(PlayerName2.Text)
	if player then
		if not LoopKill[player.UserId] then
			LoopKill[player.UserId] = {Player = player}
			game.StarterGui:SetCore("SendNotification", {
				Title = "Notification",
				Text = "Click Again To Unloop Kills "..player.DisplayName,
				Button1 = "OK"
			})
		else
			LoopKill[player.UserId] = nil
			game.StarterGui:SetCore("SendNotification", {
				Title = "Notification",
				Text = "Click Again To Loop Kills "..player.DisplayName,
				Button1 = "OK"
			})
		end
	end
	while wait() do
		for i,v in pairs(LoopKill) do
			pcall(function()
				if v.Player then
					if v.Player.Character.Humanoid.Health > 0 then
						Kill(v.Player)
					end
				end
			end)
		end
	end
end)

KillAuraNeutral.MouseButton1Click:Connect(function()
	if States.KillAura_Neutral == true then
		States.KillAura_Neutral = false
		KillAuraNeutral.Text = "X"
	else
		States.KillAura_Neutral = true
		KillAuraNeutral.Text = ""
	end
	while wait() do
		if States.KillAura_Neutral then
			for i,v in pairs(game.Teams.Neutral:GetPlayers()) do
				pcall(function()
					if v ~= game.Players.LocalPlayer then
						if (v.Character.HumanoidRootPart.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude < 15 then
							for i = 1,25 do
								coroutine.wrap(function()
									game.ReplicatedStorage.meleeEvent:FireServer(v)	
								end)()
							end
						end
					end
				end)
			end
		end
	end
end)

